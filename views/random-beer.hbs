<div class='random-beer-container'>
  {{#each beersFromApi}}
    <div class='random-beer-card'>
      <img src='{{this.image_url}}' alt='{{this.name}}' />
      <h3>{{this.name}}</h3>
      <p>{{this.tagline}}</p>
      <p>{{this.description}}</p>
      <p>{{this.food_pairing}}</p>
      <p>{{this.brewer_tips}}</p>
    </div>
  {{/each}}
</div>